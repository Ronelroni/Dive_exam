<p id="notice"><%= notice %></p>

<h1>Property List</h1>

<%= link_to 'New Property', new_property_path %>

<table>
  <thead>
    <tr>
      <th>Property</th>
      <th>Rent</th>
      <th>Building age</th>
      <th>Adress</th>
      <th>Remark</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.property %></td>
        <td><%= property.rent %></td>
        <td><%= property.building_age %></td>
        <td><%= property.adress %></td>
        <td><%= property.remark %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


